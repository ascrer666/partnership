/* Reset */
* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

html {
    font-size: 16px;
    scroll-behavior: smooth;
}

body {
    font-family: 'Poppins', sans-serif;
    color: var(--dark-gray);
    background: var(--light-gray);
    line-height: 1.6;
    display: flex;
    flex-direction: column;
    min-height: 100vh;
}

.page-wrapper {
    flex: 1 0 auto;
    display: flex;
    flex-direction: column;
}

section {
    padding: 4rem 0;
}

section + section {
    padding-top: 3.5rem;
}

.container {
    width: min(92%, var(--container-width));
    margin: 0 auto;
}

.section-title {
    font-size: clamp(2rem, 2.8vw, 2.6rem);
    font-weight: 600;
    text-align: center;
    margin-bottom: 2.5rem;
}

.section-title-left {
    font-size: clamp(1.9rem, 2.5vw, 2.3rem);
    font-weight: 600;
    margin-bottom: 1.6rem;
}

p {
    margin-bottom: 1rem;
    color: var(--dark-gray);
}
/* Header & Navigation */
.header {
    position: fixed;
    inset: 0 0 auto 0;
    background: rgba(255, 255, 255, 0.94);
    backdrop-filter: blur(14px);
    box-shadow: 0 6px 24px rgba(0, 0, 0, 0.08);
    z-index: 1000;
}

.nav-container {
    width: min(92%, var(--container-width));
    margin: 0 auto;
    padding: 0.9rem 0;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.logo-img {
    height: 52px;
    width: auto;
}

.nav-menu {
    display: flex;
    align-items: center;
    list-style: none;
    gap: 1.75rem;
    transition: var(--transition);
}

.nav-link {
    color: var(--dark-gray);
    font-weight: 500;
    text-decoration: none;
    position: relative;
    transition: var(--transition);
}

.nav-link::after {
    content: '';
    position: absolute;
    left: 0;
    bottom: -0.4rem;
    width: 0;
    height: 2px;
    background: var(--primary-turquoise);
    transition: var(--transition);
}

.nav-link:hover {
    color: var(--primary-turquoise);
}

.nav-link:hover::after {
    width: 100%;
}

.whatsapp-btn {
    display: inline-flex;
    align-items: center;
    gap: 0.45rem;
    padding: 0.45rem 1.05rem;
    border-radius: 22px;
    background: #25D366;
    color: var(--white);
    text-decoration: none;
    font-size: 0.85rem;
    font-weight: 500;
    box-shadow: 0 8px 18px rgba(37, 211, 102, 0.25);
    transition: var(--transition);
}

.whatsapp-btn:hover {
    background: #1baa52;
    transform: translateY(-1px);
}

.hamburger {
    display: none;
    flex-direction: column;
    gap: 6px;
    width: 34px;
    height: 30px;
    justify-content: center;
    cursor: pointer;
}

.hamburger span {
    display: block;
    height: 2px;
    background: var(--dark-gray);
    transition: var(--transition);
    border-radius: 999px;
}
/* Hero */
.hero {
    position: relative;
    min-height: 72vh;
    display: flex;
    align-items: center;
    padding-top: 6rem;
    background: var(--dark-gray);
}

.hero-video-bg {
